@model List<Carro>

<h1>Carros Cadastrados</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Modelo</th>
            <th scope="col">Ano</th>
            <th scope="col">Valor</th>
            <th scope="col">Tipo</th>
            <th scope="col">Teto Solar</th>
        </tr>
    </thead>
    <tbody>
        @{
            var i = 0;
            foreach (Carro carro in TempData["lista"] as List<Carro>)
            {
                i++;
                <tr>
                    <th scope="row">@i</th>
                    <td>@carro.Modelo</td>
                    <td>@carro.Ano</td>
                    <td>@carro.Valor</td>
                    <td>@carro.Tipo</td>
                    <td>@carro.TetoSolar</td>
                </tr>
            }
        }
    </tbody>
</table>
